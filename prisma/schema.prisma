generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model courses {
  id          String   @id @default(uuid())
  createdAt   DateTime @default(now())
  name        String
  description String
  coverImage  String
  featured    Boolean  @default(false)
}
